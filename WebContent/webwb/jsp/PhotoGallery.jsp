<script>
		showSlides("gallerySlides", 0);
		showSlides("gallerySlides1", 0);
		showSlides("gallerySlides2", 0);
		showSlides("gallerySlides3", 0);
		showSlides("gallerySlides4", 0);
		showSlides("gallerySlides5", 0);
		function showSlides(sliderName, slideIndex) {
			var i;
			var slides = document.getElementsByClassName(sliderName);
			for (i = 0; i < slides.length; i++) {
				slides[i].style.display = "none";
			}
			slideIndex++;
			if (slideIndex > slides.length) {
				slideIndex = 1
			}
			slides[slideIndex - 1].style.display = "block";
			setTimeout(showSlides, 2000, sliderName, slideIndex); // Change image every 2 seconds
		}
	</script>
